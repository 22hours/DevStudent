type User{
    email : String
    password : String
    nickname : String
    schoolName : String
    date : String
    authState : String
    accessToken : String
    gitLink : String
}
extend type Mutation{
    createUser(email: String!, password: String!, nickname: String!, schoolName : String!) : User
    updateUserAuthState(authState : String!) : User
    loginToServer(email : String, password : String) : User
    checkDuplicateNickname(nickname : String) : Count
    checkDuplicateEmail(email : String) : Count
    logoutFromServer(email : String) : Count
    updateUserInfo(nickname : String, gitLink : String) : User
}

extend type Query{
    findUserByNickname(nickname : String) : User
}