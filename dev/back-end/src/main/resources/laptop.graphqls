type Laptop{
    id: Int
    name: String!
    price : Int
    ram : Int
}
type Query{
    findAllLaptops:[Laptop]!
    findLaptopByID(id:Int):Laptop
    findLaptopsLessThan(price:Int):[Laptop]!
}

type Mutation{
    deleteLaptop(id: Int):Laptop
    createLaptop(id : Int, name:String!, price:Int, ram:Int):Laptop
}